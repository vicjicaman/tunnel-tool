{
  "name": "@nebulario/tunnel-local-web",
  "version": "1.70.8-initial-client-prod",
  "main": "index.js",
  "repository": "git@github.com:vicjicaman/tunnel-local-web.git",
  "author": "Victor Jimenez <vic@repoflow.com>",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "clean:server": "rm -Rf ./dist/server*",
    "clean:web": "rm -Rf ./dist/web*",
    "build:prod:server": "yarn clean:server && webpack --config webpack.config.server.js --mode=production ",
    "build:dev:server": "yarn clean:server && webpack --config webpack.config.server.js --mode=development ",
    "build:prod:web": "yarn clean:web && webpack --config webpack.config.web.js --mode=production ",
    "build:dev:web": "yarn clean:web && webpack --config webpack.config.web.js --mode=development ",
    "build:dev": "run-p \"build:dev:*\" ",
    "build:prod": "run-p \"build:prod:*\" ",
    "start:dev": "nodemon ./dist/index.js ",
    "start:prod": "node ./dist/index.js ",
    "build:watch:dev": "run-p \"build:dev:* --watch\" ",
    "build:watch:prod": "run-p \"build:prod:* --watch\" "
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
    "@nebulario/tunnel-layout": "1.70.0-master",
    "@nebulario/tunnel-logger": "1.70.0-master",
    "@nebulario/tunnel-utils": "1.70.0-master",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.5.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "graphql": "^14.5.4",
    "jquery": "^3.5.0",
    "node-fetch": "^2.6.0",
    "react": "^16.9.0",
    "react-apollo": "^3.1.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.4.1",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "socket.io-client": "^2.3.0",
    "validator": "^12.0.0"
  }
}